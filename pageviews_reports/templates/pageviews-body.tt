
<table class="b">

  [% headers = data.shift %] 
  [% FOREACH header IN headers %]
  <th>[% header %] </th>
  [% END %]

  [% WHILE (row = data.shift) %]
    <tr>
    <td> [% row.shift %] </td>
      <script language='javascript'>
        [% FOREACH cell IN row %]
          tdc('#3BFF3B','+[% cell.monthly_delta %]%, [% cell.percentage_of_monthly_total %]%, 1st','[% cell.monthly_count %]&nbsp;M');
        [% END %] 
      </script>
    </tr>
  [% END %]

</table>

