{
  "model"                : "parallel",
  "max-children"         : 10,
  "input-path"           : "/a/squid/archive/sampled",
  "children-output-path" : "/tmp/pageviews-restricted/map",
  "output-path"          : "/tmp/pageviews-restricted",
  "output-formats"       : ["wikireport","web","json"],
  "logs-prefix"          : "sampled",
  "restrictions"         : {
    "days-of-each-month"   : [2,3],
    "lines-for-each-day"   : 100000
  },
  "start"    : {
    "year"   : 2012,
    "month"  : 8
  },
  "end"      : {
    "year"   : 2013,
    "month"  : 3
  }
}
